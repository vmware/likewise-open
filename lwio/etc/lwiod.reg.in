[HKEY_THIS_MACHINE\Services]

[HKEY_THIS_MACHINE\Services\lwio]
"Description"="Likewise IO Manager"
"Path"="@SBINDIR@/lwiod"
"Arguments"="@SBINDIR@/lwiod --syslog"
"Environment"=""
"Dependencies"="lwreg"
"Type"=dword:00000001

[HKEY_THIS_MACHINE\Services\rdr]
"Description"="Likewise CIFS Redirector"
"Path"="@providerdir@/librdr.sys@MOD_EXT@"
"Arguments"="rdr"
"Environment"=""
"Dependencies"="lwio"
"Type"=dword:00000003

[HKEY_THIS_MACHINE\Services\npfs]
"Description"="Likewise Named Pipe Filesystem"
"Path"="@providerdir@/libnpfs.sys@MOD_EXT@"
"Arguments"="npfs"
"Environment"=""
"Dependencies"="lwio"
"Type"=dword:00000003

[HKEY_THIS_MACHINE\Services\lwio\Parameters]
"SignMessagesIfSupported"=dword:00000000

[HKEY_THIS_MACHINE\Services\lwio\Parameters\Drivers]
"Load"="rdr,npfs"

[HKEY_THIS_MACHINE\Services\lwio\Parameters\Drivers\npfs]
"Path"="@providerdir@/libnpfs.sys@MOD_EXT@"

[HKEY_THIS_MACHINE\Services\lwio\Parameters\Drivers\rdr]
"Path"="@providerdir@/librdr.sys@MOD_EXT@"

[HKEY_THIS_MACHINE\Services\lwio\Parameters\Drivers\iotest]
"Path"="@providerdir@/libiotest.sys@MOD_EXT@"

[HKEY_THIS_MACHINE\Software\Likewise\lwiosrvstat]
"Target"="file"
"Path"="/tmp/lwiosrvstat.log"
