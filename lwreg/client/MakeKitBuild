make()
{
    CLIENT_SOURCES="\
        clientipc.c \
        regclient.c \
        regntclient.c"

    mk_library \
        LIB=regclient \
        SOURCES="$CLIENT_SOURCES" \
        INCLUDEDIRS=". ../include" \
        LIBDEPS="regcommon lwmsg lwmsg_nothr" \
        HEADERDEPS="lw/base.h lwmsg/lwmsg.h"
}
