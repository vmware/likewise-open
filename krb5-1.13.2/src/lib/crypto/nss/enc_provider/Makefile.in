mydir=lib$(S)crypto$(S)nss$(S)enc_provider
BUILDTOP=$(REL)..$(S)..$(S)..$(S)..
CRYPTO_IMPL_CFLAGS=@CRYPTO_IMPL_CFLAGS@
LOCALINCLUDES = -I$(srcdir)/../../krb -I$(srcdir)/.. $(CRYPTO_IMPL_CFLAGS)

STLIBOBJS= \
	enc_gen.o 	\
	des.o 	\
	des3.o 	\
	rc4.o 	\
	aes.o 	\
	camellia.o

OBJS= \
	$(OUTPRE)enc_gen.$(OBJEXT) 	\
	$(OUTPRE)des.$(OBJEXT) 	\
	$(OUTPRE)des3.$(OBJEXT) 	\
	$(OUTPRE)aes.$(OBJEXT) 	\
	$(OUTPRE)camellia.$(OBJEXT) 	\
	$(OUTPRE)rc4.$(OBJEXT)

SRCS= \
	$(srcdir)/enc_gen.c 	\
	$(srcdir)/des.c 	\
	$(srcdir)/des3.c 	\
	$(srcdir)/aes.c 	\
	$(srcdir)/camellia.c 	\
	$(srcdir)/rc4.c

all-unix:: all-libobjs

includes:: depend

depend:: $(SRCS)

clean-unix:: clean-libobjs

@libobj_frag@

